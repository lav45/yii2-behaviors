{
    "name": "lav45/yii2-behaviors",
    "description": "This is a set of Yii2 behaviors extensions",
    "keywords": [
        "yii2",
        "extension",
        "behavior"
    ],
    "homepage": "https://github.com/lav45/yii2-behaviors",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "2.0.*"
    },
    "require-dev": {
        "cweagans/composer-patches": "^1.7",
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "4.8.34",
        "ext-pdo_sqlite": "*"
    },
    "autoload": {
        "psr-4": {
            "lav45\\behaviors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lav45\\behaviors\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6.x-dev"
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "phpunit/phpunit": {
                "Fix PHP 7 compatibility": "https://yiisoft.github.io/phpunit-patches/phpunit_php7.patch",
                "Fix PHP 8 compatibility": "https://yiisoft.github.io/phpunit-patches/phpunit_php8.patch"
            }
        }
    }
}
